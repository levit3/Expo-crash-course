{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/index.jsx",
    "app/(tabs)/_layout.jsx",
    "app/(tabs)/bookmark.jsx",
    "app/(tabs)/create.jsx",
    "app/(tabs)/home.jsx",
    "app/(tabs)/profile.jsx",
    "app/search/[query].jsx",
    "app/_layout.jsx",
    "app/+html.jsx",
    "app/+not-found.jsx"
  ]
}
